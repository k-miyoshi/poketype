{
  "name": "pokemon-type-chart",
  "version": "1.0.0",
  "description": "Type chart for Pokemon GO",
  "main": "index.html",
  "scripts": {
    "build": "webpack",
    "prod": "webpack --config webpack-production.config.js",
    "sass": "node-sass --output-style compressed src/sass/poketype.scss docs/css/poketype.css",
    "sprite": "node_modules/.bin/spritesmith",
    "eslint": "node_modules/.bin/eslint ./src/js/* ./src/js/**/*",
    "start": "npm run sprite && npm run sass && npm run prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k-miyoshi/pokemon-type-chart.git"
  },
  "author": "k-miyoshi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/k-miyoshi/pokemon-type-chart/issues"
  },
  "homepage": "https://github.com/k-miyoshi/pokemon-type-chart#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "eslint": "^4.4.1",
    "eslint-plugin-react": "^7.2.0",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "spritesmith-cli": "^1.1.2",
    "webpack": "^3.5.3"
  },
  "dependencies": {
    "flipsnap": "^0.6.3",
    "react": "^15.5.0",
    "react-dom": "^15.3.0"
  }
}
